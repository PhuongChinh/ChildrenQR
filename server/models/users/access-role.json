{
  "name": "AccessRole",
  "plural": "roles",
  "base": "Role",
  "idInjection": false,
  "forceId": true,
  "strict": true,
  "options": {
    "validateUpsert": true,
    "mysql": {
      "table": "access_role"
    }
  },
  "postgresql": {
    "schema": "public",
    "table": "access_role"
  },
  "properties": {
    "group": {
      "type": "string",
      "description": "Group Name to sort of the roles",
      "postgresql": {
        "dataType": "character varying",
        "dataLength": 256
      },
      "default": "system"
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "AccessUser",
      "foreignKey": "roleId",
      "through": "AccRoleMapping",
      "keyThrough": "principalId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}